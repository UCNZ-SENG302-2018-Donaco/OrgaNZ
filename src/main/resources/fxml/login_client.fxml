<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng302.Controller.Client.ClientLoginController">
  <children>
    <Button mnemonicParsing="false" onAction="#goBack" prefHeight="30.0" prefWidth="120.0" style="-fx-base: rgba(173,216,230, 0.8);" text="Back">
      <VBox.margin>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
      </VBox.margin>
    </Button>
    <BorderPane VBox.vgrow="ALWAYS">
      <top>
        <Label text="Client Login" BorderPane.alignment="CENTER">
          <font>
            <Font size="24.0" />
          </font>
          <BorderPane.margin>
            <Insets bottom="10.0" />
          </BorderPane.margin>
        </Label>
      </top>
      <center>
        <ListView fx:id="clientList" maxWidth="250.0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
          <BorderPane.margin>
            <Insets />
          </BorderPane.margin>
        </ListView>
      </center>
      <bottom>
        <Button mnemonicParsing="false" onAction="#signIn" prefHeight="30.0" prefWidth="100.0" text="Log in" BorderPane.alignment="CENTER" defaultButton="true">
          <BorderPane.margin>
            <Insets bottom="30.0" top="15.0" />
          </BorderPane.margin>
        </Button>
      </bottom>
    </BorderPane>
  </children>
</VBox>
