<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
  minWidth="-Infinity" prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121"
  fx:controller="com.humanharvest.organz.controller.client.SubmitDeathDetailsController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="25.0" prefWidth="100.0"/>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0"/>
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
  </rowConstraints>
  <children>
    <Label GridPane.columnIndex="1" text="Details of Death">
      <font>
        <Font size="24.0"/>
      </font>
    </Label>
    <Label GridPane.halignment="CENTER" GridPane.rowIndex="1" text="Date"/>
    <DatePicker GridPane.columnIndex="1" GridPane.rowIndex="1" maxWidth="1.7976931348623157E308"
      fx:id="deathDatePicker"/>
    <Label GridPane.halignment="CENTER" GridPane.rowIndex="2" text="Time"/>
    <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" promptText="HH:mm:ss" fx:id="deathTimeField"/>
    <Label GridPane.halignment="CENTER" GridPane.rowIndex="3" text="Country"/>
    <ChoiceBox GridPane.columnIndex="1" GridPane.rowIndex="3" maxWidth="1.7976931348623157E308" prefHeight="25.0"
      fx:id="deathCountry"/>
    <Label GridPane.halignment="CENTER" GridPane.rowIndex="4" text="Region"/>
    <ChoiceBox GridPane.columnIndex="1" GridPane.rowIndex="4" maxWidth="1.7976931348623157E308" prefHeight="25.0"
      fx:id="deathRegionCB"/>
    <TextField GridPane.columnIndex="1" GridPane.rowIndex="4" fx:id="deathRegionTF"/>
    <Label GridPane.halignment="CENTER" GridPane.rowIndex="5" text="City"/>
    <TextField GridPane.columnIndex="1" GridPane.rowIndex="5" fx:id="deathCity"/>
    <GridPane GridPane.columnIndex="1" GridPane.rowIndex="6" hgap="20.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <children>
        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0"
          text="Cancel"/>
        <Button GridPane.columnIndex="1" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#submit"
          prefHeight="25.0" text="Submit"/>
      </children>
    </GridPane>
  </children>
</GridPane>
